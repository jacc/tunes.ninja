{
  "name": "tunes.ninja",
  "version": "3.0.0",
  "repository": "https://github.com/jacc/tunes.ninja.git",
  "author": "Jack LaFond <hi@jack.link>",
  "scripts": {
    "dev": "cross-env NODE_ENV=development ts-node-dev --exit-child src/index.ts",
    "build": "tsc",
    "start": "cross-env NODE_ENV=production node ./dist",
    "generate": "prisma generate --schema ../prisma/schema.prisma",
    "lint": "eslint .",
    "services:up": "docker-compose -f ../docker-compose.dev.yml up -d&&yarn migrate --schema ../prisma/schema.prisma",
    "services:down": "docker-compose -f docker-compose.dev.yml down",
    "services:start": "docker-compose -f docker-compose.dev.yml start",
    "services:stop": "docker-compose -f docker-compose.dev.yml stop",
    "migrate": "prisma migrate dev",
    "migrate:deploy": "prisma migrate deploy --schema .prisma/schema.prisma",
    "up:prod": "docker-compose up --build -d",
    "down:prod": "docker-compose down",
    "prod": "yarn migrate:deploy && yarn start"
  },
  "dependencies": {
    "@prisma/client": "^3.9.2",
    "@types/node-schedule": "^1.3.2",
    "axios": "^0.26.0",
    "cross-env": "^7.0.3",
    "discord.js": "^14.0.0-dev.1653480262-68d5169",
    "dotenv": "^10.0.0",
    "graphql": "^16.3.0",
    "graphql-request": "^4.0.0",
    "install": "^0.13.0",
    "ioredis": "^4.28.0",
    "mytls": "^1.0.5",
    "node-fetch": "^2.6.6",
    "node-schedule": "^2.0.0",
    "prisma": "^3.4.2",
    "signale": "^1.4.0",
    "ts-node": "^10.8.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.2"
  },
  "devDependencies": {
    "@types/ioredis": "^4.28.1",
    "@types/signale": "^1.4.2"
  }
}
